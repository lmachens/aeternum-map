{
  "name": "aeternum-map",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --ignore-watch src/app --project tsconfig.server.json src/server",
    "client:build": "vite build",
    "server:build": "tsc --project tsconfig.server.json && cp -R src/assets dist/server/assets",
    "build": "npm run server:build && npm run client:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server/server.js"
  },
  "devDependencies": {
    "@overwolf/types": "^3.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/leaflet": "^1.7.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.13",
    "@types/node-fetch": "^3.0.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2",
    "vite": "^2.5.3"
  },
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "isomorphic-fetch": "^3.0.0",
    "leaflet": "^1.7.1",
    "leaflet-canvas-markers": "^1.0.7",
    "markdown-to-jsx": "^7.1.3",
    "mongodb": "^4.1.1",
    "multer": "^1.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tilelayer-canvas": "^1.1.3"
  }
}
